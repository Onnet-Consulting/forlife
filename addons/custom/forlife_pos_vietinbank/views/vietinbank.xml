<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="vietinbank_transaction_view_tree" model="ir.ui.view">
            <field name="name">vietinbank_transaction_view_tree</field>
            <field name="model">vietinbank.transaction.model</field>
            <field name="arch" type="xml">
                <tree string="vietinbank_transaction_tree" limit="80" class="list_table_custom">
                    <field name="effect_date"/>
                    <field name="customer_name"/>
                    <field name="debit_account"/>
                    <field name="service_bank"/>
                    <field name="amount"/>
                    <field name="ref"/>
                    <field name="ref_no"/>

                </tree>
            </field>
        </record>
    </data>
</odoo>