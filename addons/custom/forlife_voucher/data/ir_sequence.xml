<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="program_voucher_sequence" model="ir.sequence">
            <field name="name">Program Voucher</field>
            <field name="code">program.voucher</field>
            <field name="active">TRUE</field>
            <field name="prefix">A</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">001</field>
        </record>

        <record id="voucher_voucher_serial_sequence_2" model="ir.sequence">
            <field name="name">Serial Voucher</field>
            <field name="code">voucher.voucher.serial</field>
            <field name="active">TRUE</field>
            <field name="prefix">%s%(y)s</field>
            <field name="padding">10</field>
            <field name="number_next">1</field>
            <field name="number_increment">001</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>