<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">
    <record id="expense_category_rule" model="ir.rule">
        <field name="name">Expense category multi company rule</field>
        <field name="model_id" ref="model_expense_category"/>
        <field eval="True" name="global"/>
        <field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
    </record>
     
    <record id="expense_item_rule" model="ir.rule">
        <field name="name">Expense item multi company rule</field>
        <field name="model_id" ref="model_expense_item"/>
        <field eval="True" name="global"/>
        <field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
    </record>
</data>
</odoo>
