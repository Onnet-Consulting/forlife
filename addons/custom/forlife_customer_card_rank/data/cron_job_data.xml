<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="ir_cron_action_inactive_member_card_program" model="ir.cron">
            <field name="name">Inactive Member Card Program</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field ref="model_member_card" name="model_id"/>
            <field name="nextcall" eval="(DateTime.now().replace(hour=17, minute=0, second=0)).strftime('%Y-%m-%d %H:%M:%S')" />
            <field name="state">code</field>
            <field name="code">model.action_inactive_member_card_program()</field>
        </record>

    </data>
</odoo>