<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="res_field_value_report_view_tree" model="ir.ui.view">
            <field name="name">res_field_value_report_view_tree</field>
            <field name="model">res.field.value.report</field>
            <field name="arch" type="xml">
                <tree string="res_field_value_report_form" editable="top">
                    <field name="name" required="1"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="res_field_value_report_view_filter" model="ir.ui.view">
            <field name="name">res_field_value_report_view_filter</field>
            <field name="model">res.field.value.report</field>
            <field name="arch" type="xml">
                <search string="res_field_value_report_view_filter">
                    <field name="name"/>
                    <field name="description"/>
                </search>
            </field>
        </record>

        <record id="res_field_value_report_action" model="ir.actions.act_window">
            <field name="name">Giá trị dữ liệu</field>
            <field name="res_model">res.field.value.report</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem id="res_field_value_report_menu" parent="root_permission_report_menu" action="res_field_value_report_action" sequence="15"/>

    </data>
</odoo>