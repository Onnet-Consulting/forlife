<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum41Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum41MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum41MainTemplate">
        <table t-att-id="widget.report_type_id" class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-set="format_decimal" t-value="widget.func.format_decimal"/>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <td class="index-report"><t t-esc="line.stt"/></td>
                        <td><t t-esc="line.ma_lenh_sx"/></td>
                        <td><t t-esc="line.kho_xuong"/></td>
                        <td class="column_nowrap"><t t-esc="line.ma_vat_tu"/></td>
                        <td class="right"><t t-esc="format_decimal(line.sl_sx_ke_hoach, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.sl_nk_thuc_te, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.sl_th_thuc_te, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.sl_vt_kho_cap, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.chenh_lech_qt, 0)"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>