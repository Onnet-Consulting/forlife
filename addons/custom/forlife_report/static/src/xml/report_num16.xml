<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum16Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum16MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum16MainTemplate">
        <table t-att-id="widget.report_type_id" class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <td><t t-esc="line.stt"/></td>
                        <td><t t-esc="line.ngay"/></td>
                        <td><t t-esc="line.so_ct"/></td>
                        <td><t t-esc="line.kho_xuat"/></td>
                        <td><t t-esc="line.so_ct2"/></td>
                        <td><t t-esc="line.kho_nhap"/></td>
                        <td><t t-esc="line.ma_khach"/></td>
                        <td><t t-esc="line.ten_khach"/></td>
                        <td><t t-esc="line.doi_tuong"/></td>
                        <td><t t-esc="line.nhom_hang"/></td>
                        <td><t t-esc="line.dong_hang"/></td>
                        <td><t t-esc="line.ket_cau"/></td>
                        <td><t t-esc="line.ma_vach"/></td>
                        <td><t t-esc="line.ma_hang"/></td>
                        <td><t t-esc="line.ten_hang"/></td>
                        <td><t t-esc="line.mau_sac"/></td>
                        <td><t t-esc="line.kich_co"/></td>
                        <td><t t-esc="line.nam_sx"/></td>
                        <td><t t-esc="line.bo_suu_tap"/></td>
                        <td><t t-esc="line.xuat_xu"/></td>
                        <td><t t-esc="line.subclass1"/></td>
                        <td><t t-esc="line.subclass2"/></td>
                        <td><t t-esc="line.subclass3"/></td>
                        <td><t t-esc="line.subclass4"/></td>
                        <td><t t-esc="line.subclass5"/></td>
                        <td><t t-esc="line.dv_tinh"/></td>
                        <td><t t-esc="line.nhap"/></td>
                        <td><t t-esc="line.xuat"/></td>
                        <td><t t-esc="line.don_gia"/></td>
                        <td><t t-esc="line.ma_loai"/></td>
                        <td><t t-esc="line.ngay_to_khai"/></td>
                        <td><t t-esc="line.dien_giai"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>