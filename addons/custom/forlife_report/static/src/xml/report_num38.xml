<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum38Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum38MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum38MainTemplate">
        <table t-att-id="widget.report_type_id"
               class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-set="index" t-value="0"/>
                <t t-foreach="options.data" t-as="line">
                    <t t-set="index" t-value="index + 1"/>
                    <tr>
                        <td><t t-esc="index"/></td>
                        <td><t t-esc="line.ten"/></td>
                        <td><t t-esc="line.ten_nb"/></td>
                        <td><t t-esc="line.ma_kho"/></td>
                        <td><t t-esc="line.ma_nb"/></td>
                        <td><t t-esc="line.nhom_kho"/></td>
                        <td><t t-esc="line.loai_kho"/></td>
                        <t t-if="widget.column_add">
                            <td><t t-esc="line.dia_diem"/></td>
                            <td><t t-esc="line.ma_dia_diem"/></td>
                            <td><t t-esc="line.loai_dia_diem"/></td>
                            <td><t t-esc="line.kieu_dia_diem"/></td>
                            <td><t t-esc="line.ky_gui"/></td>
                        </t>
                        <td><t t-esc="line.dia_chi"/></td>
                        <td><t t-esc="line.congty"/></td>
                        <td><t t-esc="line.thuong_hieu"/></td>
                        <td><t t-esc="line.dien_tich"/></td>
                        <td><t t-esc="line.dien_thoai"/></td>
                        <td><t t-esc="line.quan_ly"/></td>
                        <td><t t-esc="line.trang_thai"/></td>
                        <td><t t-esc="line.kv_kinhdoanh"/></td>
                        <td><t t-esc="line.kv_dialy"/></td>
                        <td><t t-esc="line.kv_thoitiet"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>