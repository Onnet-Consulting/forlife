<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum43Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum43MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum43MainTemplate">
        <table t-att-id="widget.report_type_id" class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-set="format_decimal" t-value="widget.func.format_decimal"/>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <td class="index-report"><t t-esc="line.num"/></td>
                        <td class="column_nowrap"><t t-esc="line.ngay_tt"/></td>
                        <td><t t-esc="line.ma_ch"/></td>
                        <td class="column_nowrap"><t t-esc="line.ten_ch"/></td>
                        <td><t t-esc="line.phien_bh"/></td>
                        <td><t t-esc="line.so_dh"/></td>
                        <td class="column_nowrap"><t t-esc="line.ten_kh"/></td>
                        <td class="column_nowrap"><t t-esc="line.dien_giai"/></td>
                        <td><t t-esc="format_decimal(line.so_tien, 2)"/></td>
                        <td><t t-esc="line.hinhthuc_tt"/></td>
                        <td><t t-esc="line.tt_phien"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>