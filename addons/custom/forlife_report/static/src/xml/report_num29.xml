<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum29Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum29MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum29MainTemplate">
        <table t-att-id="widget.report_type_id" class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <td><t t-esc="line.phan_loai"/></td>
                        <td><t t-esc="line.ma"/></td>
                        <td><t t-esc="line.ten"/></td>
                        <td><t t-esc="line.ma_luu_kho"/></td>
                        <td><t t-esc="line.ten_luu_kho"/></td>
                        <td><t t-esc="line.trang_thai"/></td>
                        <td><t t-esc="line.dvt"/></td>
                        <td><t t-esc="line.sl"/></td>
                        <td><t t-esc="line.nhan_vien"/></td>
                        <td><t t-esc="line.bo_phan"/></td>
                        <td><t t-esc="line.vi_tri"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>