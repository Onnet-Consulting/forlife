<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum39Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum39MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum39MainTemplate">
        <table t-att-id="widget.report_type_id" class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-set="format_decimal" t-value="widget.func.format_decimal"/>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <td class="index-report"><t t-esc="line.stt"/></td>
                        <td><t t-esc="line.ngay"/></td>
                        <td class="column_nowrap"><t t-esc="line.so_ct"/></td>
                        <td><t t-esc="line.kho_nhap"/></td>
                        <td class="column_nowrap"><t t-esc="line.ma_doi_tuong"/></td>
                        <td><t t-esc="line.ten_doi_tuong"/></td>
                        <td><t t-esc="line.thuong_hieu"/></td>
                        <td><t t-esc="line.nhom_hang"/></td>
                        <td><t t-esc="line.dong_hang"/></td>
                        <td><t t-esc="line.ket_cau"/></td>
                        <td class="column_nowrap"><t t-esc="line.ma_vach"/></td>
                        <td class="column_nowrap"><t t-esc="line.ma_hang"/></td>
                        <td><t t-esc="line.ten_hang"/></td>
                        <td><t t-esc="line.dv_tinh"/></td>
                        <td class="right"><t t-esc="format_decimal(line.sl_nhap, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.gt_nhap, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.cp_truoc_thue, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.thue_nhap_khau, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.thue_tieu_thu_db, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.cp_sau_thue, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.cp_npl, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.cp_nc, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.tong, 0)"/></td>
                        <td class="column_nowrap"><t t-esc="line.tk_hach_toan"/></td>
                        <td><t t-esc="line.dien_giai"/></td>
                        <td><t t-esc="line.so_po_tham_chieu"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>