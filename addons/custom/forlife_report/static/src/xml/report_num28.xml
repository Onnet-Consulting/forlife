<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum28Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum28MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum28MainTemplate">
        <table t-att-id="widget.report_type_id" class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <td><t t-esc="line.ten"/></td>
                        <td><t t-esc="line.ngay_tao_po"/></td>
                        <td><t t-esc="line.ngay_du_kien"/></td>
                        <td><t t-esc="line.ghi_chu"/></td>
                        <td><t t-esc="line.kho"/></td>
                        <td><t t-esc="line.so_phieu_kho"/></td>
                        <td><t t-esc="line.num"/></td>
                        <td><t t-esc="line.barcode"/></td>
                        <td><t t-esc="line.sl_nhu_cau"/></td>
                        <td><t t-esc="line.ten_sp"/></td>
                        <td><t t-esc="line.mau"/></td>
                        <td><t t-esc="line.size"/></td>
                        <td><t t-esc="line.dvt"/></td>
                        <td><t t-esc="line.sl_nhan"/></td>
                        <td><t t-esc="line.phan_do_dang"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>