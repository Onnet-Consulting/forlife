<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum13Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum13MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum13MainTemplate">
        <table t-att-id="widget.report_type_id" class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-set="format_decimal" t-value="widget.func.format_decimal"/>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <td><t t-esc="line.num"/></td>
                        <td><t t-esc="line.pr_name"/></td>
                        <td><t t-esc="line.pr_date"/></td>
                        <td><t t-esc="line.po_name"/></td>
                        <td><t t-esc="line.po_date"/></td>
                        <td><t t-esc="line.suppliers_name"/></td>
                        <td><t t-esc="line.sku_code"/></td>
                        <td><t t-esc="line.barcode"/></td>
                        <td><t t-esc="line.product_name"/></td>
                        <td class="right"><t t-esc="format_decimal(line.product_qty, 2)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.price_unit, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.discount_percent, 3)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.price_subtotal, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.qty_received, 2)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.qty_not_received, 2)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.qty_invoiced, 2)"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>