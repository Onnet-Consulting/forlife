<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum24Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum24MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum24MainTemplate">
        <table t-att-id="widget.report_type_id" class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-set="format_decimal" t-value="widget.func.format_decimal"/>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <td><t t-esc="line.stt"/></td>
                        <td><t t-esc="line.ma_cua_hang"/></td>
                        <td><t t-esc="line.ten_cua_hang"/></td>
                        <td><t t-esc="format_decimal(line.tich, 0)"/></td>
                        <td><t t-esc="format_decimal(line.tieu, 0)"/></td>
                        <td><t t-esc="format_decimal(line.tra, 0)"/></td>
                        <td><t t-esc="format_decimal(line.hoan, 0)"/></td>
                        <td><t t-esc="format_decimal(line.bu, 0)"/></td>
                        <td><t t-esc="format_decimal(line.reset, 0)"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>