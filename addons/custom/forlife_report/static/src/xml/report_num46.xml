<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum46Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum46MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum46MainTemplate">
        <table t-att-id="widget.report_type_id" class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-set="format_decimal" t-value="widget.func.format_decimal"/>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <td class="index-report"><t t-esc="line.stt"/></td>
                        <td class="column_nowrap"><t t-esc="line.ngay_ct"/></td>
                        <td class="column_nowrap"><t t-esc="line.so_ct"/></td>
                        <td class="column_nowrap"><t t-esc="line.ma_dt"/></td>
                        <td><t t-esc="line.ten_dt"/></td>
                        <td><t t-esc="line.dien_giai"/></td>
                        <td class="column_nowrap"><t t-esc="line.tk_doi_ung"/></td>
                        <td class="right"><t t-esc="format_decimal(line.ps_no, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.ps_co, 0)"/></td>
                        <td><t t-esc="line.ma_vv"/></td>
                        <td><t t-esc="line.ma_bp"/></td>
                        <td><t t-esc="line.ten_bp"/></td>
                        <td><t t-esc="line.khoan_muc_cp"/></td>
                        <td><t t-esc="line.so_lsx"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>