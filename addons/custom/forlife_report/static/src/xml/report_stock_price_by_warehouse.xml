<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportStockPriceByWarehouse" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportStockPriceByWarehouseMain"/>
        </xpath>
    </t>
    <t t-name="ReportStockPriceByWarehouseMain">
        <table style="">
            <thead>
                <tr>
                    <th>Mã SP</th>
                    <th>Tên SP</th>
                    <th>Tồn</th>
                </tr>
            </thead>
            <tbody>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <span class="product_id report_hidden"><t t-esc="line.product_id"/></span>
                        <td><t t-esc="line.product_barcode"/></td>
                        <td><t t-esc="line.product_name"/></td>
                        <td><t t-esc="line.quantity"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>