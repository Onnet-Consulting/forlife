<?xml version="1.0" encoding="utf-8" ?>
<template xml:space="preserve">
    <t t-name="ReportNum11Template" t-inherit="ReportBase" t-inherit-mode="primary">
        <xpath expr="//div[@id='main-content']" position="inside">
            <t t-call="ReportNum11MainTemplate"/>
        </xpath>
    </t>

    <t t-name="ReportNum11MainTemplate">
        <table t-att-id="widget.report_type_id" class="o_list_table table table-sm table-hover mb-0 o_list_table_ungrouped table-striped report_table">
            <t t-call="ReportTitle"/>
            <tbody>
                <t t-set="format_decimal" t-value="widget.func.format_decimal"/>
                <t t-foreach="options.data" t-as="line">
                    <tr>
                        <td><t t-esc="num"/></td>
                        <td><t t-esc="line.store_name"/></td>
                        <td><t t-esc="line.customer_info[0]"/></td>
                        <td><t t-esc="line.customer_info[1]"/></td>
                        <td><t t-esc="line.customer_info[2]"/></td>
                        <td><t t-esc="line.customer_info[3]"/></td>
                        <td><t t-esc="line.ngay_mua_dk"/></td>
                        <td><t t-esc="line.ngay_mua_gn"/></td>
                        <td><t t-esc="line.ngay_ck"/></td>
                        <td class="right"><t t-esc="format_decimal(line.dt_ps, 0)"/></td>
                        <td class="right"><t t-esc="format_decimal(line.min_turnover - line.dt_ps, 0)"/></td>
                        <td><t t-esc="line.current_rank"/></td>
                        <td><t t-esc="line.new_rank"/></td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</template>