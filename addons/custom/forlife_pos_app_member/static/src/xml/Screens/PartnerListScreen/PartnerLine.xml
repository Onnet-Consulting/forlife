<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

     <t t-name="PartnerLine" t-inherit="point_of_sale.PartnerLine" t-inherit-mode="extension" owl="1">
        <xpath expr="//td[hasclass('partner-line-email')]" position="after">
            <div class="partner-line-type" >
                <t t-set="name" t-value="_getPartnerType()"/>
                <t t-esc="name"/>
            </div>
        </xpath>
    </t>

    <t t-name="PartnerListScreen" t-inherit="point_of_sale.PartnerListScreen" t-inherit-mode="extension" owl="1">
        <xpath expr="//th[hasclass('partner-line-email')]" position="after">
            <th t-if="!env.isMobile" class="partner-line-type">Loại KH lẻ</th>
        </xpath>
    </t>
 </templates>
