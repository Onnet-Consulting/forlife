<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    	 <record id="view_picking_type_form_forlife_inherit" model="ir.ui.view">
            <field name="name">Operation Types inherit</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='return_picking_type_id']" position="after">
                    <field name="other_picking_type_id"/>
                </xpath>

                <xpath expr="//field[@name='default_location_dest_id']" position="after">
                    <field name="other_location_id"/>
                </xpath>

                
            </field>
        </record>
    </data>
</odoo>