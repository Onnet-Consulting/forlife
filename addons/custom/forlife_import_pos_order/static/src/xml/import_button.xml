<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="import.pos.order.buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="extension" owl="1">
        <xpath expr="//*[@class='btn btn-secondary fa fa-download o_list_export_xlsx']" position="after">
            <button type="object" t-if="props.resModel == 'pos.order'"
                    class="btn btn-primary import_your_action"
                    t-ref="action"
                    action="forlife_import_pos_order.pos_order_import_view_form_action"
                    style="margin-left:10px;" t-on-click="buttonImport">
                Nhập từ excel
            </button>
        </xpath>
    </t>
</templates>