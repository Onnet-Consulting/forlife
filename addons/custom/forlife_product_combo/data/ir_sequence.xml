<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="0">
        <record id="product_combo_sequence" model="ir.sequence">
            <field name="name">Combo Sequence</field>
            <field name="code">product.combo</field>
            <field name="active">TRUE</field>
            <field name="prefix">MB</field>
            <field name="padding">7</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <data noupdate="0">
        <record id="check_wait_confirm_stock_transfer_request" model="ir.cron">
            <field name="name">Overdue transfer warning</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field ref="model_stock_transfer_request" name="model_id"/>
            <field name="state">code</field>
            <field name="code">model.check_wait_confirm_stock_transfer_request()</field>
        </record>
    </data>
</odoo>