<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form_nhanh_connector" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.nhanh_connector</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="41"/>
            <field name="inherit_id" ref="account.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                 <xpath expr="//div[@id='quick_edit_mode']" position="after">
                    <h2>Nhanh Config Parameter</h2>
                    <div class="row mt16 o_settings_container" id="nhanh_config_parameter">
                        <div class="col-12 col-lg-12 o_setting_box">
                            <group>
                                <field name="nhanh_company_id"/>
                            </group>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
