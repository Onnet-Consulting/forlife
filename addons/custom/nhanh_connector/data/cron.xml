<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_sync_nhanhvn_order" model="ir.cron">
        <field name="name">Sync Order Nhanhvn</field>
        <field name="model_id" ref="model_sale_order"/>
        <field name="state">code</field>
        <field name="code">model.start_sync_order_from_nhanh()</field>
        <field name="interval_number">15</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall"/>
    </record>
    <record id="ir_cron_sync_nhanhvn_customer" model="ir.cron">
        <field name="name">Sync Customer Nhanhvn</field>
        <field name="model_id" ref="model_sale_order"/>
        <field name="state">code</field>
        <field name="code">model.start_sync_customer_from_nhanh()</field>
        <field name="interval_number">15</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall"/>
    </record>
    <record id="ir_cron_sync_update_price_product_nhanhvn" model="ir.cron">
        <field name="name">Sync Update price poduct Nhanhvn</field>
        <field name="model_id" ref="model_promotion_campaign"/>
        <field name="state">code</field>
        <field name="code">model.start_sync_price_product_to_nhanh()</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall"/>
    </record>
